{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "skill_id": "compute/numerical-analysis",
  "input": {
    "type": "object",
    "properties": {
      "capability": {
        "type": "string",
        "enum": [
          "find_zeta_zeros",
          "verify_zero",
          "riemann_siegel",
          "zero_spacing_analysis",
          "compute_zeta_high_precision"
        ],
        "default": "find_zeta_zeros"
      },
      "t_min": {
        "type": "number",
        "description": "Minimum imaginary part to search"
      },
      "t_max": {
        "type": "number",
        "description": "Maximum imaginary part to search"
      },
      "t_value": {
        "type": "number",
        "description": "Imaginary part of s = 1/2 + it"
      },
      "s_real": {
        "type": "number",
        "description": "Real part of s"
      },
      "s_imag": {
        "type": "number",
        "description": "Imaginary part of s"
      },
      "precision": {
        "type": "integer",
        "description": "Decimal precision for mpmath"
      },
      "num_zeros": {
        "type": "integer",
        "description": "Number of zeros to analyze"
      },
      "start_t": {
        "type": "number",
        "description": "Starting value for zero search"
      }
    },
    "required": ["capability"]
  },
  "output": {
    "type": "object",
    "properties": {
      "success": {
        "type": "boolean"
      },
      "output": {
        "type": "object"
      },
      "error": {
        "type": "string"
      }
    }
  }
}
